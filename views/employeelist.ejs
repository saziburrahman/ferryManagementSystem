<div id="employeeList" style="min-height: 100vh;background-image: url(image/ferry_table.jpg);background-repeat: no-repeat;background-size: cover;overflow: hidden;width: 100%;">
    <h1 style="text-align: center;color: black;margin-top: 30px;">Employee Information</h1>
    <div >
        <table class="table" style="color: black;margin-top: 30px; width: 80%;margin: auto;">
            <thead>
                <tr>
                    <th>ID</th>
                    <th>Name</th>
                    <th>Address</th>
                    <th>Email</th>
                    <th>Join Date</th>
                    <th>Contact</th>
                    <th>Gender</th>
                    <th></th>
                    <th></th>
                    <th></th>
                </tr>
            </thead>
            <tbody>
                <% if(employee.length>0){ %>
                    <% for(let i=0;i<employee.length;i++){ %>
                        <tr>
                            <td><%= i %></td>
                            <td><%= employee[i].nameInput %></td>
                            <td><%= employee[i].address %></td>
                            <td><%= employee[i].email %></td>
                            <td><%= employee[i].joinDate.toDateString() %></td>
                            <td><%= employee[i].contactNumber %></td>
                            <td><%= employee[i].gender %></td>
                            <td><a href="/employeeEdit/<%= employee[i].id %>" class="btn btn-primary">Edit</a></td>
                            <td><a href="/emplyeeDelete/<%= employee[i].id %>" class="btn btn-danger">Delete</a></td>
                            <td><a href="/profileIndex/<%= employee[i].id %>" class="btn btn-primary">Profile</a></td>
            
                        </tr>
                    <%}%>
                <%}%>
             
            </tbody>
    
        </table>
        <div class="row" style="margin-top: 10px;">
