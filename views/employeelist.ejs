            <div class="col" style="min-width: 30%;"></div>
            <div class="col" style="min-width: 30%;text-align: end;"><a href="/employeeRegistration" class="btn btn-primary me-md-5" style="padding: 15px 20px;">Add Employee</a>
            </div>
        </div>
    </div>
